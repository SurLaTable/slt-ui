	"scripts": {
		"build": "babel-node --config-file ./builder/config/babel.config.js ./builder/build.js",
		"docs": "babel-node --config-file ./builder/config/babel.config.js ./builder/docs/serve.config.js --dev",
		"jest": "jest --coverage --updateSnapshot && codecov",
		"prettier": "npx prettier-eslint --list-different --config ./prettier.config.js --eslint-config-path ./.eslintrc.json --write \"{,!(.git|*build|coverage|*dist|docs|node_modules|styleguid*)/**/}*.{css,js,json,jsx,less,md,sass,scss}\"",
		"prepare-release": "yarn install && yarn test && yarn build && yarn prettier",
		"sandbox": "babel-node --config-file ./builder/config/babel.config.js ./DEPRECATED_sandbox/serve.config.js --dev",
		"start-component": "babel-node --config-file ./builder/config/babel.config.js ./builder/start-component.js",
		"tasks": "yarn build --tasks",
		"test": "yarn jest"
	},
