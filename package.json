{
	"name": "convert-material-ui-to-async",
	"version": "1.0.0",
	"description": "",
	"main": "run.js",
	"config": {
		"commit": "v1.0.0-beta.38"
	},
	"scripts": {
		"build": "yarn prep:material && yarn transform",
		"build:clean": "yarn reset && yarn build",
		"reset": "cd material-ui && cross-var git reset --hard $npm_package_config_commit && cd ..",
		"transform": "node ./transforms/index.js",
		"test": "yarn test:transforms && yarn test:material",
		"test:transforms": "./node_modules/.bin/mocha --require babel-register ./transforms/*.spec.js",
		"test:material": "cd material-ui && yarn test:karma && cd ..",
		"test:1": "cd material-ui && cross-env NODE_ENV=test mocha src/{,**/}*.spec.js",
		"prep:material": "cd material-ui && yarn install && yarn add --dev babel-plugin-dynamic-import-node -W && yarn add react-async-component -W && cd .."
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/slt_development/convert-material-ui-to-async.git"
	},
	"keywords": [
		"jscodeshift"
	],
	"author": "SLT",
	"license": "GPL-2.0+",
	"homepage": "https://bitbucket.org/slt_development/convert-material-ui-to-async#readme",
	"dependencies": {
		"jscodeshift": "^0.5.0"
	},
	"devDependencies": {
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-register": "^6.26.0",
		"cross-env": "^5.1.4",
		"cross-var": "^1.1.0",
		"glob": "^7.1.2",
		"mocha": "^5.0.4"
	}
}
