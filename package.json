{
	"name": "@sur-la-table/slt-ui",
	"version": "0.0.3",
	"description": "Sur La Table UI Library",
	"main": "index.js",
	"config": {
		"commit": "v1.0.0-beta.38"
	},
	"scripts": {
		"build": "yarn prep:material && yarn transform",
		"build:clean": "yarn reset && yarn build",
		"prepare": "yarn build:clean",
		"prep:material":
			"cd material-ui && yarn install && yarn add --dev babel-plugin-dynamic-import-node -W && yarn add react-async-component -W && cd ..",
		"postinstall":
			"git submodule update --init --recursive && cd material-ui && cross-var git checkout $npm_package_config_commit && cd ..",
		"prettier":
			"npx prettier --config .prettierrc.json --write \"{,!(.git|*build|*dist|material-ui|node_modules|styleguide)/**/}*.{css,js,json,jsx,less,md,sass,scss}\"",
		"reset":
			"cd material-ui && cross-var git reset --hard $npm_package_config_commit && cd ..",
		"transform": "node ./transforms/index.js",
		"test": "yarn test:transforms && yarn test:material",
		"test:transforms":
			"./node_modules/.bin/mocha --require babel-register ./transforms/*.spec.js",
		"test:material": "cd material-ui && yarn test:karma && cd ..",
		"test:1":
			"cd material-ui && cross-env NODE_ENV=test mocha src/{,**/}*.spec.js"
	},
	"repository": {
		"type": "git",
		"url":
			"git+ssh://git@bitbucket.org/slt_development/convert-material-ui-to-async.git"
	},
	"keywords": ["jscodeshift"],
	"author": "SLT",
	"license": "GPL-2.0+",
	"homepage":
		"https://bitbucket.org/slt_development/convert-material-ui-to-async#readme",
	"dependencies": {
		"@types/jss": "^9.3.0",
		"@types/react-transition-group": "^2.0.6",
		"babel-runtime": "^6.26.0",
		"brcast": "^3.0.1",
		"classnames": "^2.2.5",
		"deepmerge": "^2.0.1",
		"dom-helpers": "^3.2.1",
		"hoist-non-react-statics": "^2.5.0",
		"jss": "^9.3.3",
		"jss-camel-case": "^6.0.0",
		"jss-default-unit": "^8.0.2",
		"jss-global": "^3.0.0",
		"jss-nested": "^6.0.1",
		"jss-props-sort": "^6.0.0",
		"jss-vendor-prefixer": "^7.0.0",
		"keycode": "^2.1.9",
		"lodash": "^4.2.0",
		"normalize-scroll-left": "^0.1.2",
		"prop-types": "^15.6.0",
		"react-async-component": "^1.0.2",
		"react-event-listener": "^0.5.1",
		"react-jss": "^8.1.0",
		"react-lifecycles-compat": "^1.0.2",
		"react-popper": "^0.8.0",
		"react-scrollbar-size": "^2.0.2",
		"react-transition-group": "^2.2.1",
		"recompose": "^0.26.0",
		"scroll": "^2.0.3",
		"warning": "^3.0.0"
	},
	"devDependencies": {
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-register": "^6.26.0",
		"cross-env": "^5.1.4",
		"cross-var": "^1.1.0",
		"glob": "^7.1.2",

		"jscodeshift": "^0.5.0",

		"mocha": "^5.0.4"
	}
}
