{
  "name": "@sur-la-table/slt-ui",
  "version": "0.0.29",
  "description": "Sur La Table UI Library",
  "main": "./index.js",
  "config": {
    "commit": "v1.0.0-beta.40"
  },
  "files": ["material-ui/build"],
  "scripts": {
    "build":
      "yarn prebuild && yarn prepare:material && yarn transform && cd ./material-ui && yarn build && cd .. && node ./scripts/setup-release.js",
    "prebuild": "yarn git-reset && rimraf build",
    "prepare:material":
      "cd material-ui && yarn install && yarn add --dev babel-plugin-dynamic-import-node babel-plugin-syntax-dynamic-import -W && yarn add react-async-component -W && cd ..",
    "prettier":
      "npx prettier --config ./prettier.config.js --write \"{,!(.git|*build|*dist|material-ui|node_modules|styleguide)/**/}*.{css,js,json,jsx,less,md,sass,scss}\"",
    "transform": "node ./transforms/index.js",
    "test": "yarn test:transforms",
    "test:transforms": "./node_modules/.bin/mocha --require babel-register ./transforms/*.spec.js",
    "git-setup":
      "git submodule update --init --recursive && cd material-ui && cross-var git checkout $npm_package_config_commit && cd ..",
    "git-reset":
      "cd material-ui && git reset --hard origin/master && git pull origin master && cross-var git reset --hard $npm_package_config_commit && cd ..",
    "git-mark-release": "cross-var git tag -a $npm_package_version -m '$npm_package_version'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/slt_development/slt-ui.git"
  },
  "keywords": ["component", "library", "jscodeshift"],
  "author": "SLT",
  "license": "GPL-2.0+",
  "homepage": "https://bitbucket.org/slt_development/slt-ui/",
  "dependencies": {
    "@types/jss": "^9.3.0",
    "@types/react-transition-group": "^2.0.6",
    "babel-runtime": "^6.26.0",
    "brcast": "^3.0.1",
    "classnames": "^2.2.5",
    "deepmerge": "^2.0.1",
    "dom-helpers": "^3.2.1",
    "hoist-non-react-statics": "^2.5.0",
    "jss": "^9.3.3",
    "jss-camel-case": "^6.0.0",
    "jss-default-unit": "^8.0.2",
    "jss-global": "^3.0.0",
    "jss-nested": "^6.0.1",
    "jss-props-sort": "^6.0.0",
    "jss-vendor-prefixer": "^7.0.0",
    "keycode": "^2.1.9",
    "lodash": "^4.2.0",
    "normalize-scroll-left": "^0.1.2",
    "prop-types": "^15.6.0",
    "react-async-component": "^1.0.2",
    "react-event-listener": "^0.5.1",
    "react-jss": "^8.1.0",
    "react-lifecycles-compat": "^1.0.2",
    "react-popper": "^0.8.0",
    "react-scrollbar-size": "^2.0.2",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "scroll": "^2.0.3",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cli-highlight": "^1.2.3",
    "cross-env": "^5.1.4",
    "cross-var": "^1.1.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "jscodeshift": "^0.5.0",
    "mocha": "^5.0.4",
    "prettier": "^1.11.1"
  }
}
